ALGORITHM algorithm_name
VAR
    tab1 : ARRAY_OF INTEGER[3]; 
    tab2 : ARRAY_OF INTEGER[4];
    i : INTEGER;
    j : INTEGER;
BEGIN
    tab1 := [3,1,7,9];
    tab2 := [2,4,1,9,3];
    sum1 = 0;
    sum2 = 0;
    FOR (i=0;i<3;i++)
        sum1= sum1 + tab1[i];
    END_FOR
    write(sum1);
    FOR (i=0;i<4;i++)
        sum2= sum2 + tab2[i];
    END_FOR
    write(sum2);
END